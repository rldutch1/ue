&#xD;&#xD;
where ea.encntr_id = $enc_id;72958204.00;$enc_id;&#xD;&#xD;
and ea.encntr_alias_type_cd in (1077.00, 1079.00);FIN Number and MRN Number&#xD;&#xD;
detail&#xD;&#xD;
case (ea.encntr_alias_type_cd)&#xD;&#xD;
