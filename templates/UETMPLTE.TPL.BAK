  create procedure show_storedprocedures as&#xD;&#xD;
  SELECT &#xD;&#xD;
    ROUTINE_SCHEMA,&#xD;&#xD;
    ROUTINE_NAME&#xD;&#xD;
  FROM INFORMATION_SCHEMA.ROUTINES&#xD;&#xD;
  WHERE ROUTINE_TYPE = 'PROCEDURE'&#xD;&#xD;
