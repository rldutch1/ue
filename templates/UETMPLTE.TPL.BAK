	2 returned_encounter_type = vc ; Assiging a name and data type to a value returned from the select statement.&#xD;&#xD;
	2 returned_mrn = vc ;&#xD;&#xD;
	2 returned_fin = vc ;&#xD;&#xD;
&#xD;&#xD;
&#xD;&#xD;
%i cclsource:status_block.inc&#xD;&#xD;
)&#xD;&#xD;
